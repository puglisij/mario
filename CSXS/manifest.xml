<?xml version='1.0' encoding='UTF-8'?>
<ExtensionManifest ExtensionBundleId="com.mario" ExtensionBundleVersion="1.0.0" ExtensionBundleName="mario" Version="7.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ExtensionList>
    <Extension Id="com.mario.panel" Version="1.0.0" />
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="PHXS" Version="[18,99.9]" />
    </HostList>
    <LocaleList>
      <Locale Code="All" />
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="7.0" />
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.mario.panel">
      <DispatchInfo>
        <Resources>
          <!-- <MainPath>./public/index-dev.html</MainPath> -->
          <MainPath>./dist/index.html</MainPath>
          <ScriptPath>./public/index.jsx</ScriptPath>
          <CEFCommandLine>
            <Parameter>--enable-nodejs</Parameter>
            <Parameter>--mixed-context</Parameter>
          </CEFCommandLine>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>Mario</Menu>
          <Geometry>
            <Size>
              <Height>600</Height>
              <Width>400</Width>
            </Size>
            <MaxSize>
              <Height>1080</Height>
              <Width>1920</Width>
            </MaxSize>
            <MinSize>
              <Height>400</Height>
              <Width>400</Width>
            </MinSize>
          </Geometry>
          <Icons />
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>
