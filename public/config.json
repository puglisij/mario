{
    "pauseAfterEveryAction": false, 
    "pauseAfterEveryImage": false,
    "pauseOnExceptions": true,
    "watchRoot": "C:/image-processing",
    "watchers": [
        {
            "target": "/Process",
            "extensions": ["psd", "tif", "jpg", "jpeg", "png"],
            "classifiers": ["Product", "Ad"]
        },
        {
            "target": "/ProcessWeb",
            "extensions": ["tif", "png"],
            "classifiers": ["Product"]
        }
    ]
}